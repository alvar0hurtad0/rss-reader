{
	"name": "rss-reader",
	"displayName": "RSS Reader",
	"description": "Add a RSS feed widget to VSCode",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "RSS",
					"title": "RSS",
					"icon": "resources/icons/treeView.svg"
				}
			]
		},
		"views": {
			"RSS": [
				{
					"id": "RSS-1",
					"name": "RSS-1"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "typedoc && npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"doc": "typedoc"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"typed-rest-client": "^1.5.0"
	}
}
