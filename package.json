{
	"name": "rss-reader",
	"displayName": "RSS-Reader",
	"description": "RSS feed widget",
	"version": "0.0.1",
	"publisher": "ekarbe",
	"author": "Eike Christian Karbe <eichkarbe@yahoo.de>",
	"license": "MIT",
	"engines": {
		"vscode": "^1.40.0"
	},
	"bugs": {
		"url": "https://github.com/ekarbe/rss-reader/issues",
		"email": "eichkarbe@yahoo.de"
	},
	"homepage": "https://ekarbe.github.io/rss-reader",
	"repository": {
		"type": "git",
		"url": "https://github.com/ekarbe/rss-reader.git"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"rss",
		"news",
		"browse",
		"feed",
		"widget"
	],
	"galleryBanner": {
		"color": "#EE8030",
		"theme": "dark"
	},
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "RSSReader.Open",
				"title": "Open links from RSS Reader"
			},
			{
				"command": "RSSReader.Refresh",
				"title": "Refresh the RSS feed",
				"icon": {
					"light": "resources/icons/light/refresh.svg",
					"dark": "resources/icons/dark/refresh.svg"
				}
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "RSS",
					"title": "RSS",
					"icon": "resources/icons/treeView.svg"
				}
			]
		},
		"views": {
			"RSS": [
				{
					"id": "RSS-1",
					"name": "RSS-1"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "RSSReader.Refresh",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "RSSReader.Open"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "typedoc && npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"doc": "typedoc"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"typed-rest-client": "^1.5.0"
	}
}